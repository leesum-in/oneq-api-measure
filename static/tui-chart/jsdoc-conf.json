{
    "source": {
        "include": ["src/js", "README.md"],
        "exclude": [
            "src/js/code-snippet-util.js"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles" : true
        },
        "applicationName": "Chart",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "recurse": true,
        "name": "Chart",
        "destination": "Output Location",
        "tutorials": "samples"
    }
}
